  object QPERSONEL_LOOP: TOraQuery
    SQL.Strings = (
      'SELECT "ADI SOYADI",RES FROM'
      
        '(SELECT P.ADI||'#39' '#39'||P.SOYADI "ADI SOYADI",'#39'KADRO'#39'||P.KUR_SIC_NO ' +
        'RES FROM KT_PERSONEL P WHERE P.KURUMDAN_AYR IS NULL AND P.SICILD' +
        'E_GORUNSUN IS NOT NULL '
      'UNION ALL'
      
        'SELECT trupper(S.ADI||'#39' '#39'||S.SOYADI) "ADI SOYADI",'#39'SIRKET'#39'||S.PE' +
        'RSONEL_ID RES FROM DOGUMKAYIT.DA_SRKT_PERSONEL S WHERE S.AYRILDI' +
        ' IS NULL)'
      'ORDER BY "ADI SOYADI"')
    Left = 312
    Top = 40
  end
  object QARACLAR: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO DOGUMKAYIT.DA_ARAC_TAKIP'
      '  (ADI_SOYADI, DAHILI_TEL, CEP_TEL, PLAKA, MODELI, FOTO)'
      'VALUES'
      '  (:ADI_SOYADI, :DAHILI_TEL, :CEP_TEL, :PLAKA, :MODELI, :FOTO)')
    SQLDelete.Strings = (
      'DELETE FROM DOGUMKAYIT.DA_ARAC_TAKIP'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLUpdate.Strings = (
      'UPDATE DOGUMKAYIT.DA_ARAC_TAKIP'
      'SET'
      
        '  ADI_SOYADI = :ADI_SOYADI, DAHILI_TEL = :DAHILI_TEL, CEP_TEL = ' +
        ':CEP_TEL, PLAKA = :PLAKA, MODELI = :MODELI, FOTO = :FOTO'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLLock.Strings = (
      'SELECT * FROM DOGUMKAYIT.DA_ARAC_TAKIP'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT DOGUMKAYIT.DA_ARAC_TAKIP.ADI_SOYADI, DOGUMKAYIT.DA_ARAC_T' +
        'AKIP.DAHILI_TEL, DOGUMKAYIT.DA_ARAC_TAKIP.CEP_TEL, DOGUMKAYIT.DA' +
        '_ARAC_TAKIP.PLAKA, DOGUMKAYIT.DA_ARAC_TAKIP.MODELI, DOGUMKAYIT.D' +
        'A_ARAC_TAKIP.FOTO FROM DOGUMKAYIT.DA_ARAC_TAKIP'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQL.Strings = (
      'select T.*,ROWID from DOGUMKAYIT.da_arac_takip t'
      'where t.adi_soyadi like :ADI||'#39'%'#39' and t.plaka like :PLAKA||'#39'%'#39
      'ORDER BY T.ADI_SOYADI')
    Left = 240
    Top = 128
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ADI'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'PLAKA'
        Value = Null
      end>
  end
